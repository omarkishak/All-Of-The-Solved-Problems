# include<iostream>
using namespace std;
int main(){
    int a[10],x=0,flag=0;

    cout<<" please enter 10 numbers";
    for(int i=0;i<10;i++){ 
        cin>>a[i];}

    cout<<" please enter x";
    cin>>x;

    for(int i=0;i<10;i++){
        if(a[i]==x) {
            cout << "the location is =" << i ;
            flag=1;
        }
    }
    if(flag== 0)
    {
        cout << "not found";
    }
    system("pause");
}
