# include<iostream>
using namespace std;

int main(){
    int a[10],b[10],c[10];

    cout<<" please enter 10 numbers of first array";
    for(int i=0;i<10;i++){
        cin>>a[i];
    }
    cout<<" please enter 10 numbers of second array";
    for(int i=0;i<10;i++){
        cin>>b[i];
    }
    for(int i=0;i<10;i++){
        if(a[i]==b[i]){
            c[i]=0;
        }
        else if(a[i]>b[i]){
            c[i]= a[i];
        }
        else {
            c[i]= b[i];
        }
        cout<<c[i]<<endl;
    }
    return 0;
}
