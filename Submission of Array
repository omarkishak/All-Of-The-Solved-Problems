#include <iostream>
#include <cmath>
#include <sstream>
using namespace std;

int main(){
    double arr[5], userInput, arrSum = 0;

    cout << "Enter 5 numbers: " << endl;

    for(int i = 0; i < 5; i++) {
        cin >> userInput;
        arr[i] = userInput;
    }

    cout << "The sum of all numbers is: ";
    for(int i = 0; i < 5; i++) {
        arrSum += arr[i];
    }

    cout << arrSum;
    
    return 0;
}
